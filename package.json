{
  "name": "vs-jshell",
  "displayName": "vs-jshell",
  "description": "JShell integration for VSCode",
  "repository": {
    "type": "git",
    "url": "https://github.com/LuceResearchLab/vscode-jshell.git"
  },
  "publisher": "Lugano Computing Education Research Lab",
  "maintainers": [
    {
      "name": "Joey Bevilacqua",
      "email": "bevilj@usi.ch"
    }
  ],
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Java",
    "JShell"
  ],
  "bugs": {
    "url": "https://github.com/LuceResearchLab/vscode-jshell/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "terminal": {
      "profiles": [
        {
          "id": "vs-jshell:terminal-profile",
          "title": "JShell"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.23.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
